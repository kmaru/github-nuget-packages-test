<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>github_nuget_packages_test2</RootNamespace>
    <PackageProjectUrl>https://github.com/kmaru/github-nuget-packages-test</PackageProjectUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageId>github_nuget_packages_test2</PackageId>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <!--minver-->
    <MinVerTagPrefix>v</MinVerTagPrefix>
    <MinVerDefaultPreReleasePhase>preview</MinVerDefaultPreReleasePhase>
    <RepositoryUrl>https://github.com/kmaru/github-nuget-packages-test</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="minver" Version="2.3.1">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>
</Project>
